<h1>Sign in</h1>

<p><%= notice %></p>

<%= form_tag sessions_url do %>
  <%= label_tag :email %>
  <%= email_field_tag :email %>

  <%= label_tag :password %>
  <%= password_field_tag :password %>

  <%#= label_tag :organization %>
  <%#= select_tag(:organization, options_from_collection_for_select(Organization.all)) %>
  <br>
  <%= link_to "Forgot Password?", new_password_reset_path %>
  <br>
  <%= submit_tag "Sign in", class: 'btn btn-success' %>
<% end %>

