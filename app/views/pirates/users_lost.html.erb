<p>Current Quarter = <%= @start_date %></p>

<table>
<tr>
<td>Culprit's Name</td>
<td>Culprit's Email</td>
</tr>
<% @person_quarters.each do |pq| %>
<% person = Person.find(pq.person_id) %>
<tr>
<td><%= "#{person.first_name} #{person.last_name} " %></td>
<td><%= person.email %></td>
</tr>
<% end %>

</table>